<section class="form">
  <%= form_for [:admin, @station] do |f| %>
  <section class="form-input">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </section>
  <section class="form-input">
    <%= f.label :dock_count %>
    <%= f.number_field :dock_count %>
  </section>
  <section class="form-input">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </section>
  <section class="form-input">
    <%= f.label :installation_date %>
    <%= f.date_field :installation_date %>
  </section>
  <section class="form-input">
    <%= f.submit %>
  </section>
  <% end %>
</section>
