<% @trips.each do |trip| %>
  Trip ID: <%= trip.id %> <br>
  <%= trip.duration %> <br>
  <%= trip.start_date %> <br>
  <%= trip.start_station.name %> <br>
  <%= trip.end_date %> <br>
  <%= trip.end_station.name %> <br>
  <%= trip.bike_id %> <br>
  <%= trip.subscription_type %> <br>
  <%= trip.zip_code %> <br>
<% end %>
<%= paginate @trips %> <br>
