<% @trips.each do |trip| %>
  Trip ID: <%= trip.id %> <br>
  <%= trip.duration %> <br>
  <%= trip.start_date %> <br>
  <%= trip.start_station.name %> <br>
  <%= trip.end_date %> <br>
  <%= trip.end_station.name %> <br>
  <%= trip.bike_id %> <br>
  <%= trip.subscription_type %> <br>
  <%= trip.zip_code %> <br>
  <%= link_to "More Info?", trip_path(trip) %>
  <%= link_to "Edit Trip", edit_admin_trip_path(trip) if current_admin? %>
  <%= link_to "Delete Trip", admin_trip_path(trip), method: "delete" if current_admin? %>
<% end %>
<%= paginate @trips %> <br>
